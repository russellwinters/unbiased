{
  "name": "unbiased-monorepo",
  "version": "2.0.0",
  "private": true,
  "workspaces": [
    "V1",
    "V2"
  ],
  "scripts": {
    "dev:v1": "npm run dev --workspace=V1",
    "dev:v2": "npm run dev --workspace=V2",
    "build:v1": "npm run build --workspace=V1",
    "build:v2": "npm run build --workspace=V2",
    "server:v1": "npm run server --workspace=V1",
    "client:v1": "npm run client --workspace=V1",
    "start:v1": "npm start --workspace=V1",
    "start:v2": "npm start --workspace=V2"
  },
  "description": "Unbiased news aggregator - V1 (legacy) and V2 (modern)",
  "repository": {
    "type": "git",
    "url": "https://github.com/russellwinters/unbiased.git"
  },
  "author": "Russell Winters",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
